diff --new-file --recursive --context org-5.19a/remove org-work/remove
*** org-5.19a/remove	Wed Dec 31 16:00:00 1969
--- org-work/remove	Sat Jan 26 14:46:18 2008
***************
*** 0 ****
--- 1,24 ----
+ #!/bin/sh
+ # @PREFIX@/lib/emacsen-common/packages/remove/org-el
+ set -e
+ 
+ FLAVOR=$1
+ PACKAGE="org-el"
+ 
+ if [ -z "${FLAVOR}" ]; then
+   echo Need argument to determin FLAVOR of emacsen
+   exit 1
+ fi
+ if [ -z "${PACKAGE}" ]; then
+   echo Internal error: need package name
+   exit 1
+ fi
+ if [ "X${FLAVOR}" = Xemacs ]; then exit 0; fi
+ 
+ ELCDIR=@PREFIX@/share/${FLAVOR}/site-lisp/${PACKAGE}
+ 
+ /bin/echo "remove/${PACKAGE}: Handling removal of emacsen flavor ${FLAVOR} ...\c"
+ rm -rf ${ELCDIR}
+ echo " done."
+ 
+ exit 0
